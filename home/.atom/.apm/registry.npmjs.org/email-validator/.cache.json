mbiance/email-validator)\n","maintainers":[{"name":"sembiance","email":"robert@cosmicrealms.com"}],"time":{"modified":"2016-11-29T18:05:17.477Z","created":"2012-06-17T21:42:45.844Z","0.1.0":"2012-06-17T21:42:46.402Z","0.1.1":"2012-06-18T10:38:19.054Z","0.1.2":"2012-08-31T02:43:49.830Z","1.0.0":"2013-12-25T04:06:45.922Z","1.0.1":"2014-07-29T00:43:29.461Z","1.0.2":"2015-07-01T00:11:54.087Z","1.0.3":"2015-08-17T17:19:52.649Z","1.0.4":"2016-01-19T14:07:19.675Z","1.0.5":"2016-08-24T17:55:23.655Z","1.0.6":"2016-09-15T15:39:37.967Z","1.0.7":"2016-09-15T19:45:21.098Z"},"author":{"name":"Robert Schultz","email":"robert@cosmicrealms.com"},"repository":{"type":"git","url":"http://github.com/Sembiance/email-validator.git"},"homepage":"http://github.com/Sembiance/email-validator","keywords":["email","validation","validator","syntax"],"bugs":{"url":"https://github.com/Sembiance/email-validator/issues"},"readmeFilename":"README.md","users":{"alxe.master":true,"ackuser":true,"kobleistvan":true,"stany":true,"monkeymonk":true,"stefano_magrassi":true,"kevincoleman":true,"princetoad":true,"bradmartin":true,"kaashin":true,"boardyuk":true,"argonlaser":true,"rwizard":true},"contributors":[{"name":"Brad Martin","email":"bradwaynemartin@gmail.com","url":"https://github.com/bradmartin"}],"_attachments":{},"_etag":"W/\"583dc35d-3bc9\"","_lastModified":"Tue, 29 Nov 2016 18:05:17 GMT"}