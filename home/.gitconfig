[user]
	name = Francesc Ros√†s
	email = francescrosasbosque@gmail.com
[color]
    ui = true
[core]
	excludesfile = ~/.gitignore
[github]
	user = frosas
[push]
	default = simple
[alias]
    ci = commit
    co = checkout
    b = branch
    fetch-all = fetch --all --prune
    merge-upstream = merge --ff-only @{u}
    s = stash save -u
    sp = stash pop
    ss = stash show -v 
    sl = stash list
    dirty-files = "!git status -s | awk '{print $2}'"
    untracked-files = ls-files -o --exclude-standard
    pretty-log = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%Creset' --abbrev-commit
    ap = add -p
    cp = checkout -p
    p = pull --ff-only
    remote-branches-by-date = for-each-ref --sort=committerdate refs/remotes --format='%(refname:short) %(committerdate:short) %(authorname)'
    web = instaweb -d webrick
    c = clean -df
    h = help
